import java.util.Scanner;
public class Evenodd{
  public static void main(String args[]){
    int num;
    Scanner n =new Scanner(System.in);
    System.out.println("Enter a integer:");
    num=n.nextInt();
    if(num%2==0){
      System.out.println("Even");
    }
    else{
      System.out.println("Odd");
    }
  }
}
