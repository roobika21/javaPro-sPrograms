package scanner;
import java.util.Scanner;
public class PRIME {
	public static void main(String args[])
	{int i,n,fact=0;
	Scanner scan=new Scanner(System.in);
	System.out.println("value of n");
	n=scan.nextInt();
	
	for(i=1;i<=n;i++)
	{
		if(n%i==0)
		{
			fact++;
		}
	}
	
	if(fact==2)
	{
		System.out.println("the given number is prime");
	}
	else
	{
		System.out.println("not prime");	
	}
	}

}
