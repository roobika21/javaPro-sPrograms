package scanner;
import java.util.Scanner;
public class STARPATTERNDAY3 {
	public static void main(String args[]) {
		int i,n,j;
		Scanner scan=new Scanner(System.in);
		System.out.println("rows");
		n=scan.nextInt();
		for(i=1;i<=n;i++)
		{
			for(j=1;j<=i;j++)
			{
				System.out.print("%");
			}
			System.out.println();
		}
	
		
	}

}
